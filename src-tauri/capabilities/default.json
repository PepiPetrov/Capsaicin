{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "sql:default",
    "sql:allow-select",
    "sql:allow-execute",
    "sql:allow-close",
    "sql:allow-load",
    "http:default",
    {
      "identifier": "http:default",
      "allow": [{ "url": "https://*" }, { "url": "http://*" }],
      "deny": []
    }
  ]
}
